{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.6.0",
      "destination": "wwwroot/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map",
        "css/bootstrap.min.css.map",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/jquery",
      "files": [
        "jquery.min.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "jquery-validate@1.19.2",
      "destination": "wwwroot/jqueryValidate",
      "files": [
        "jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/jqueryUnob",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    }

  ]
}